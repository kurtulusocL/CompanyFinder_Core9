@model User
@{
	ViewData["Title"] = "User Detail";
	ViewData["Subtitle"] = "User Navigator";
	Layout = "~/Views/Shared/_adminLayout.cshtml";
}

<div class="wrapper-pro">
	<div class="breadcome-area mg-b-30 small-dn">
		<div class="container-fluid">
			<div class="row">
				<div class="col-lg-12">
					<div class="breadcome-list map-mg-t-40-gl shadow-reset">
						<div class="row">
							<div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
								<div class="breadcome-heading">
									@ViewData["Subtitle"]
								</div>
							</div>
							<div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
								<ul class="breadcome-menu">
									<li>
										<a asp-controller="User" asp-action="kurtulusocL">User Home</a> <span class="bread-slash">/</span>
									</li>
									<li>
										<a asp-controller="User" asp-action="Admin">Admins</a> <span class="bread-slash">/</span>
									</li>
									<li>
										<a asp-controller="User" asp-action="SuspendedAdmin">Suspended Admins</a> <span class="bread-slash">/</span>
									</li>
									<li>
										<a asp-controller="User" asp-action="DeletedAdmin">Deleted Admins</a> <span class="bread-slash">/</span>
									</li>
									<li>
										<a asp-controller="User" asp-action="Company">Companies</a> <span class="bread-slash">/</span>
									</li>
									<li>
										<a asp-controller="User" asp-action="SuspendedCompany">Suspended Companies</a> <span class="bread-slash">/</span>
									</li>
									<li>
										<a asp-controller="User" asp-action="DeletedCompany">Deleted Companies</a> <span class="bread-slash">/</span>
									</li>
									<li>
										<a asp-controller="User" asp-action="UserOps">User Ops</a> <span class="bread-slash">/</span>
									</li>
								</ul>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="project-details-area mg-b-40">
		<div class="container">
			<div class="row">
				<div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
					<div class="project-details-wrap shadow-reset">
						<div class="row">
							<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
								<div class="project-details-mg">
									<div class="row">
										<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
											<div class="project-details-st">
												<span><strong>Active Status:</strong></span>
											</div>
										</div>
										<div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
											<div class="btn-group project-list-ad">
												@if (Model.IsActive == true)
												{
													<button class="btn btn-white btn-xs">Active</button>
												}
												else
												{
													<button class="btn btn-white btn-xs">DeActive</button>
												}
											</div>
										</div>
									</div>
								</div>
								<div class="project-details-mg">
									<div class="row">
										<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
											<div class="project-details-st">
												<span><strong>Deleted Status:</strong></span>
											</div>
										</div>
										<div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
											<div class="btn-group project-list-ad">
												@if (Model.IsDeleted == true)
												{
													<button class="btn btn-white btn-xs">Deleted</button>
												}
												else
												{
													<button class="btn btn-white btn-xs">Not Deleted</button>
												}
											</div>
										</div>
									</div>
								</div>
								<div class="project-details-mg">
									<div class="row">
										<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
											<div class="project-details-st">
												<span><strong>Created Date:</strong></span>
											</div>
										</div>
										<div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
											<div class="project-details-dt">
												<span>@Model.CreatedDate.ToLongDateString()</span>
											</div>
										</div>
									</div>
								</div>
								<div class="project-details-mg">
									<div class="row">
										<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
											<div class="project-details-st">
												<span><strong>Updated Date:</strong></span>
											</div>
										</div>
										<div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
											<div class="project-details-dt">
												@if (Model.UpdatedDate != null)
												{
													<span>@Model.UpdatedDate.Value.ToLongDateString()</span>
												}
												else
												{
													<span>---</span>
												}
											</div>
										</div>
									</div>
								</div>
							</div>
							<div class="col-lg-6 col-md-6 col-sm-6 col-xs-12">
								<div class="project-details-mg">
									<div class="row">
										<div class="col-lg-4 col-md-4 col-sm-4 col-xs-12">
											<div class="project-details-st">
												<span><strong>ID No:</strong></span>
											</div>
										</div>
										<div class="col-lg-8 col-md-8 col-sm-8 col-xs-12">
											<div class="project-details-dt">
												<span>@Model.Id</span>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
						<div class="row">
							<div class="col-lg-12">
								<div class="project-details-tab">
									<ul class="nav nav-tabs res-pd-less-sm">
										<li class="active">
											<a data-toggle="tab" href="#menu1">@ViewData["Title"]</a>
										</li>
									</ul>
									<div class="tab-content res-tab-content-project">
										<div id="menu1" class="tab-pane fade in active animated zoomInLeft">
											<div class="project-details-completeness">
												<dl class="table table-striped">													
													<dt class="col-sm-4">
														Name Surname
													</dt>
													<dd class="col-sm-8">
														@Model.NameSurname
													</dd>
													<dt class="col-sm-4">
														Email Address
													</dt>
													<dd class="col-sm-8">
														@Model.Email
													</dd>
													<dt class="col-sm-4">
														Username
													</dt>
													<dd class="col-sm-8">
														@Model.UserName
													</dd>
													<dt class="col-sm-4">
														Title
													</dt>
													<dd class="col-sm-8">
														@if (Model.Title != null)
														{
															@Model.Title
														}
														else
														{
															<strong style="color:darkred">No Title</strong>
														}
													</dd>
													<dt class="col-sm-4">
														Phone Number
													</dt>
													<dd class="col-sm-8">
														@Model.PhoneNumber
													</dd>
													@if (Model.Title == "User")
													{
														<dt class="col-sm-4">
															Foundation Date
														</dt>
														<dd class="col-sm-8">
															@Model.Birthdate.ToShortDateString()
														</dd>
													}
													else
													{
														<dt class="col-sm-4">
															Birthdate
														</dt>
														<dd class="col-sm-8">
															@Model.Birthdate.ToShortDateString()
														</dd>
													}
													<dt class="col-sm-4">
														Location
													</dt>
													<dd class="col-sm-8">
														@Model.City, @Model.Country
													</dd>
													<dt class="col-sm-4">
														Gender
													</dt>
													<dd class="col-sm-8">
														@if (Model.Gender != null)
														{
															@Model.Gender
														}
														else
														{
															<strong style="color:darkred">No Gender</strong>
														}
													</dd>
													<dt class="col-sm-4">
														Register Confirm Code
													</dt>
													<dd class="col-sm-8">
														@if (Model.ConfirmCode != null)
														{
															<span class="badge badge-info">@Model.ConfirmCode.Value.ToString()</span>
														}
													</dd>
													<dt class="col-sm-4">
														Is Policy Accepted
													</dt>
													<dd class="col-sm-8">
														@if (Model.IsAcceptedPolicies == true)
														{
															<strong style="color:darkblue">User Accepted to Policy</strong>
														}
														else
														{
															<strong style="color:darkred">Policy was not accepted</strong>
														}
													</dd>
													<dt class="col-sm-4">
														Normalized Username
													</dt>
													<dd class="col-sm-8">
														@Model.NormalizedUserName
													</dd>
													<dt class="col-sm-4">
														Normalized Email Address
													</dt>
													<dd class="col-sm-8">
														@Model.NormalizedEmail
													</dd>
													<dt class="col-sm-4">
														Hashed Password
													</dt>
													<dd class="col-sm-8">
														@Model.PasswordHash
													</dd>
													<dt class="col-sm-4">
														Access Failed Count
													</dt>
													<dd class="col-sm-8">
														@Model.AccessFailedCount.ToString()
													</dd>
													<dt class="col-sm-4">
														Login Counts
													</dt>
													<dd class="col-sm-8">
														@Model.UserSessions.Count()
													</dd>
													<dt class="col-sm-4">
														Counts
													</dt>
													<dd class="col-sm-8">
														<ul>
															<li>Appointments: @Model.Appointments.Count()</li>
															<li>Blogs: @Model.Blogs.Count()</li>
															<li>Cancel Memberships: @Model.CancelMemberships.Count()</li>
															<li>Companies: @Model.Companies.Count()</li>
															<li>Company Messages: @Model.CompanyMessages.Count()</li>
															<li>Comments: @Model.Comments.Count()</li>
															<li>Comment Answers: @Model.CommentAnswers.Count()</li>
															<li>Partnership: @Model.CompanyPartnerships.Count()</li>
															<li>Company Form Answer: @Model.CompanyFormAnswers.Count()</li>
															<li>Hits: @Model.Hits.Count()</li>
															<li>Likes: @Model.Likes.Count()</li>
															<li>Products: @Model.Products.Count()</li>
															<li>Profile Images: @Model.ProfileImages.Count()</li>
															<li>Questions: @Model.Questions.Count()</li>
															<li>Question Answers: @Model.QuestionAnswers.Count()</li>
															<li>Reports: @Model.Reports.Count()</li>
															<li>Saved Contents: @Model.SavedContents.Count()</li>
															<li>Tasks: @Model.ToDos.Count()</li>
														</ul>
													</dd>
												</dl>
											</div>
										</div>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>