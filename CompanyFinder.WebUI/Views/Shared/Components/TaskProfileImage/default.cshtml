@model ProfileImage
@{
	Layout = null;
}

@if (Model != null)
{
	<a>
		<img src="~/img/us/profileImage/@Model.ImageUrl" style="max-height:90px;max-width:90px;min-height:90px;min-width:90px;" />
	</a>
}
else
{
	<a>
		<img src="~/img/statics/user.png" style="max-height:90px;max-width:90px;min-height:90px;min-width:90px;" />
	</a>
}