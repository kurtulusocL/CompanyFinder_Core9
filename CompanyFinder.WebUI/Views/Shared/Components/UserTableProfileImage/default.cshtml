@model ProfileImage
@{
	Layout = null;
}

@if (Model != null)
{
	if (Model.ImageUrl != null)
	{
		<img src="~/img/us/profileImage/@Model.ImageUrl" class="img-responsive img-circle" style="max-height:90px;max-width:90px;min-height:90px;min-width:90px" asp-append-version="true" />
	}
	else
	{
		<img src="~/img/statics/user.png" class="img-responsive img-circle" style="max-height:90px;max-width:90px;min-height:90px;min-width:90px" asp-append-version="true" />
	}
}
else
{
	<img src="~/img/statics/user.png" class="img-responsive img-circle" style="max-height:90px;max-width:90px;min-height:90px;min-width:90px" asp-append-version="true" />
}