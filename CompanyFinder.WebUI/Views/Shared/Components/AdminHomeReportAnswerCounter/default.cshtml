@model ReportAnswer
@{
	Layout = null;
}

@if (ViewData["reportAnswerCount"] == null)
{
	<div class="income-rate-total">
		<div class="price-adminpro-rate">
			<h3><span class="counter">Still No Report Answer</span></h3>
		</div>
	</div>
}
else
{
	<div class="income-rate-total">
		<div class="price-adminpro-rate">
			<h3><span class="counter">@ViewData["reportAnswerCount"]</span></h3>
		</div>
	</div>
}