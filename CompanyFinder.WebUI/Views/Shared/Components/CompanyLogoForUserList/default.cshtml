@model Company
@{
	Layout = null;
}

@if (Model != null)
{
	<img src="~/img/company/logo/@Model.Logo" class="img-responsive img-circle" style="max-height:90px;max-width:90px;min-height:90px;min-width:90px" asp-append-version="true" />
}
else
{
	<img src="~/img/statics/emptyCompany.jpg" class="img-responsive img-circle" style="max-height:90px;max-width:90px;min-height:90px;min-width:90px" asp-append-version="true" />
}