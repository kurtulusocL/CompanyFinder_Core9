@model Comment
@{
	Layout = null;
}

@if (ViewData["commentCount"] == null)
{
	<div class="income-rate-total">
		<div class="price-adminpro-rate">
			<h3><span class="counter">Still No Comment</span></h3>
		</div>
	</div>
}
else
{
	<div class="income-rate-total">
		<div class="price-adminpro-rate">
			<h3><span class="counter">@ViewData["commentCount"]</span></h3>
		</div>
	</div>
}