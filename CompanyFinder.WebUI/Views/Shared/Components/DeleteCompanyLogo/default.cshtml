@model Company
@{
	Layout = null;
}

@if (Model != null)
{
	<img src="~/img/company/logo/@Model.Logo" class="img-circle circle-border m-b-md" style="max-height:100px;max-width:100px;min-height:100px;min-width:100px;" asp-append-version="true">
}
else
{
	<img src="~/img/statics/emptyCompany.jpg" class="img-circle circle-border m-b-md" style="max-height:100px;max-width:100px;min-height:100px;min-width:100px;" asp-append-version="true">
}