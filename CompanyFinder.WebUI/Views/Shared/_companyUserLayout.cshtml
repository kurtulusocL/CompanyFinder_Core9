<!doctype html>
<html lang="tr">
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <title>bi2bi.com | @ViewData["Title"]</title>
    @await Component.InvokeAsync("IconLogo")
  	<link rel="stylesheet" href="~/temp/web/css/animate.css" />
	<link rel="stylesheet" href="~/temp/web/css/flaticon.css" />
	<link rel="stylesheet" href="~/temp/web/css/bootstrap.min.css" />
	<link href="~/css/pagedlist.css" rel="stylesheet" />
	<link rel="stylesheet" href="~/temp/web/css/owl.carousel.min.css" />
	<link rel="stylesheet" href="~/temp/web/css/owl.theme.default.min.css" />
	<link rel="stylesheet" href="~/temp/web/css/lightbox.css" />
	<link rel="stylesheet" href="~/temp/web/css/skin-blue.css" />
	<link rel="stylesheet" href="~/temp/web/css/header.css" />
	<link rel="stylesheet" href="~/temp/web/css/style.css" />
	<link rel="stylesheet" href="~/temp/web/css/responsive.css" />
    <link rel="stylesheet" href="https://cdn.datatables.net/2.3.0/css/dataTables.dataTables.css" />

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
    <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
    <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
    <div class="preloader js-preloader flex-center">
        <div class="dots">
            <div class="dot"></div>
            <div class="dot"></div>
            <div class="dot"></div>
        </div>
    </div>
    <div class="wraper">
        <header class="header">
            @await Component.InvokeAsync("CompanyUserHeader")
        </header>             
      @RenderBody()
    </div>
    <footer class="inner index-2">
        <div class="bottom">
            <div class="container">
                <partial name="_companyLayoutFooterNavbar" />
            </div>
            <hr />
            <div class="container">
                <partial name="_companyLayoutFooter" />
            </div>
        </div>
    </footer>
	<script src="~/temp/web/js/jquery.min.js"></script>
	<script src="~/js/seleccion/company/companyselection.js"></script>
	<script src="~/js/seleccion/product/productselection.js"></script>
	<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>
	<script src="~/js/setdeletedalert.js"></script>
	<script src="~/js/deletealert.js"></script>
    <script src="~/temp/thirdpart/ckeditor/ckeditor.js"></script>
    <script src="~/temp/web/js/popper.min.js"></script>
    <script src="~/temp/web/js/bootstrap.min.js"></script>
    <script src="~/temp/web/js/waypoints.min.js"></script>
    <script src="~/temp/web/js/jquery.counterup.min.js"></script>
    <script src="~/temp/web/js/jquery.preloadinator.min.js"></script>
    <script>
        $('.js-preloader').preloadinator({
          minTime: 2000
        });
    </script>
    <script src="~/temp/web/js/lightbox.js"></script>
    <script src="~/temp/web/js/imagesloaded.js"></script>
    <script src="~/temp/web/js/isotope.min.js"></script>
    <script src="~/temp/web/js/owl.carousel.js"></script>
    <script src="~/temp/web/js/owl.custom.js"></script>
    <script src="~/temp/web/js/jquery.parallax-1.1.3.js"></script>
    <script src="~/temp/web/js/jquery.localscroll-1.2.7-min.js"></script>
    <script src="~/temp/web/js/jquery.smartmenus.min.js"></script>
    <script src="~/temp/web/js/custom.js"></script>
</body>
</html>