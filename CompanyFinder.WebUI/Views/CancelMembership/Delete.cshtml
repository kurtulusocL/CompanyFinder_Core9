@model CancelMembership
@{
	ViewData["Title"] = "Delete Cancel Membership";
	ViewData["Subtitle"] = "Cancel Membership Navigator";
	Layout = "~/Views/Shared/_adminLayout.cshtml";
}

<div class="wrapper-pro">
	<div class="breadcome-area mg-b-30 small-dn">
		<div class="container-fluid">
			<div class="row">
				<div class="col-lg-12">
					<div class="breadcome-list map-mg-t-40-gl shadow-reset">
						<div class="row">
							<div class="col-lg-3 col-md-3 col-sm-3 col-xs-3">
								<div class="breadcome-heading">
									@ViewData["Subtitle"]
								</div>
							</div>
							<div class="col-lg-9 col-md-9 col-sm-9 col-xs-9">
								<ul class="breadcome-menu">
									<li>
										<a asp-controller="CancelMembership" asp-action="kurtulusocL">Cancel Membership Home</a> <span class="bread-slash">/</span>
									</li>
									<li>
										<a asp-controller="CancelMembership" asp-action="Cancelled">Cancelled Membership</a> <span class="bread-slash">/</span>
									</li>
									<li>
										<a asp-controller="CancelMembership" asp-action="CancelledForAdmin">Cancelled Membership For Admin</a> <span class="bread-slash">/</span>
									</li>
									<li>
										<a asp-controller="CancelMembership" asp-action="NotCancelled">Not Cancelled Membership</a> <span class="bread-slash">/</span>
									</li>
									<li>
										<a asp-controller="CancelMembership" asp-action="CancelMembershipOps">Cancel Membership Ops</a> <span class="bread-slash">/</span>
									</li>
								</ul>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
	<div class="widgets-personal-info-area">
		<div class="container-fluid">
			<div class="row">
				<div class="col-lg-12">
					<div class="author-widgets-single widget-ov-mg-t-30 widget-ov-mg-t-n-30 widget-ov-mg-t-l-30 shadow-reset">
						<div class="persoanl-widget-hd persoanl1-widget-hd">
							<h2>
								Title: @Model.Title
							</h2>
							@if (Model.AnotherReason != null)
							{
								<h2>
									Other Reason: @Model.AnotherReason
								</h2>
							}
							else
							{
								<strong style="color:darkred">No Another Reason</strong>
							}
							<p>
								Expected Cancel Date: @Model.ExpectedCancelDate.ToShortDateString()
							</p>
							@if (Model.IsCancelled == true && Model.CancelDate != null)
							{
								<p>Cancelled at @Model.CancelDate.Value.ToShortDateString()</p>
							}
							else
							{
								<p>Not Cancelled</p>
							}
							@if (Model.UserId != null)
							{
								<p>
									<ul>
										<li>UserId: @Model.UserId</li>
										<li>User: @Model.User.NameSurname</li>
										<li>Email: @Model.User.Email</li>
										<li>Location: @Model.User.City, @Model.User.Country</li>
									</ul>
								</p>
							}
							else
							{
								<p>No User Info</p>
							}
							<div class="social-widget-result social-widget1-result">
								<span>Created At: @Model.CreatedDate.ToShortDateString()</span>
							</div>
							<div class="widget-text-box">
								<h4></h4>
								@if (Model.UpdatedDate != null)
								{
									<p>Updated At: @Model.UpdatedDate.Value.ToShortDateString()</p>
								}
								@if (Model.IsActive == true)
								{
									<p>Active</p>
								}
								else
								{
									<p>DeActive</p>
								}
								@if (Model.IsDeleted == true)
								{
									<p>Deleted</p>
								}
								else
								{
									<p>Not Deleted</p>
								}
								<div class="text-right like-love-list">
									<a href="#" onclick="confirmDelete('CancelMembership', 'DeleteCancelMembership', @Model.Id); return false;" class="btn btn-custon-rounded-three btn-danger btn-sm"> Delete </a>
								</div>
							</div>
						</div>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>